<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".Acitivity11">
    
    <ImageView
        android:layout_width="600dp"
        android:layout_height="200dp"
        android:src="@drawable/img14"
        android:scaleType="centerCrop"
        android:id="@+id/fb"></ImageView>

    <ImageView
        android:layout_width="100dp"
        android:layout_height="65dp"
        android:layout_alignEnd="@id/fb"
        android:layout_marginTop="145dp"
        android:layout_alignRight="@id/fb"
        android:src="@drawable/ic_camera"></ImageView>
    
   <ImageView
       android:layout_width="400dp"
       android:layout_height="200dp"
       android:layout_marginLeft="100dp"
       android:layout_marginRight="100dp"
       android:layout_marginTop="90dp"
       android:scaleType="centerInside"
       android:id="@+id/fb1"
       android:layout_centerHorizontal="true"
       android:src="@drawable/img12"></ImageView>

    <ImageView
        android:layout_width="90dp"
        android:layout_height="60dp"
        android:src="@drawable/ic_camera"
        android:layout_below="@id/fb"
        android:layout_marginLeft="200dp"
        android:layout_marginTop="40dp"></ImageView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sayali kadam"
        android:textSize="20sp"
        android:id="@+id/fb2"
        android:textColor="@color/black"
        android:textStyle="italic|bold"
        android:layout_below="@id/fb1"
        android:layout_marginHorizontal="120dp"></TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="She is beautiful deep down to her soul"
        android:layout_below="@id/fb2"
        android:textSize="15sp"
        android:textColor="@color/black"
        android:id="@+id/fb3"
        android:layout_centerHorizontal="true"
        android:textStyle="italic"></TextView>

    <ImageView
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:src="@drawable/img18"
        android:layout_below="@id/fb2"

        android:layout_marginLeft="305dp"></ImageView>

   <ImageView
       android:layout_width="50dp"
       android:layout_height="50dp"
       android:src="@drawable/ic_new_post"
       android:id="@+id/fb4"
       android:layout_marginLeft="10dp"
       android:layout_below="@id/fb3"></ImageView>

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_update_info"
        android:layout_below="@id/fb3"
        android:layout_marginHorizontal="120dp"></ImageView>

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_group"
        android:layout_below="@id/fb3"
        android:layout_marginHorizontal="210dp"></ImageView>

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_more"
        android:layout_below="@id/fb3"
        android:layout_marginHorizontal="290dp"></ImageView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Post"
        android:id="@+id/fb5"
        android:textColor="@color/black"
        android:layout_below="@id/fb4"
        android:layout_marginHorizontal="15dp"></TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="info"
        android:textColor="@color/black"
        android:layout_below="@id/fb4"
        android:layout_marginHorizontal="130dp"></TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add person"
        android:textColor="@color/black"
        android:layout_below="@id/fb4"
        android:layout_marginHorizontal="210dp"></TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="More"
        android:textColor="@color/black"
        android:layout_below="@id/fb4"
        android:layout_marginHorizontal="300dp"></TextView>



   <TextView
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:layout_below="@id/fb5"
       android:layout_marginTop="20dp"
       android:layout_marginLeft="50dp"
       android:textSize="17sp"
       android:textColor="@color/black"
       android:textStyle="italic"
       android:text="Studying Computer Engineering"></TextView>

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_computer"
        android:layout_below="@id/fb5"
        android:id="@+id/fb6"
        android:layout_marginTop="10dp"></ImageView>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="NSS CO-Ordinator"
        android:layout_below="@id/fb6"
        android:textSize="17sp"
        android:id="@+id/fb7"
        android:textColor="@color/black"
        android:textStyle="italic"
        android:layout_marginLeft="50dp"></TextView>

    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
       android:layout_marginBottom="30dp"
        android:layout_marginLeft="10dp"
        android:src="@drawable/img17"
      android:layout_marginTop="2dp"
        android:layout_below="@id/fb6"></ImageView>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="From Satara,Maharashtra"
        android:textStyle="italic"
        android:textSize="17sp"
        android:layout_below="@id/fb7"
        android:id="@+id/fb8"
        android:textColor="@color/black"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="50dp"></TextView>

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_location"
        android:layout_below="@id/fb7"></ImageView>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Experiencing world full of wonders"
        android:textColor="@color/black"
        android:layout_below="@id/fb8"
        android:textStyle="italic"
        android:id="@+id/fb9"
        android:layout_marginTop="10dp"
        android:textSize="17sp"
        android:layout_marginLeft="50dp"></TextView>

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_smile"
        android:layout_below="@id/fb8"></ImageView>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Good Food,Good Mood"
        android:layout_below="@id/fb9"
        android:textSize="17sp"
        android:textColor="@color/black"
        android:textStyle="italic"
        android:id="@+id/fb10"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="10dp"></TextView>

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_food"
        android:layout_below="@id/fb9"></ImageView>


<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Learn More"
    android:textColor="@color/purple_700"
    android:textStyle="bold|italic"
    android:textSize="20sp"
    android:layout_below="@id/fb10"
    android:layout_marginTop="20dp"
    android:layout_marginLeft="200dp"></TextView>






    
</RelativeLayout>